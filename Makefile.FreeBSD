.PHONY: clean
clean :
	rm -f $(APPS) fsk_fast_filter.h *.o *.a direwolf.desktop
	echo " " > tune.h


depend : $(wildcard *.c)
	makedepend -f $(lastword $(MAKEFILE_LIST)) -- $(CFLAGS) -- $^


